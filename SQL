      "cell_type": "code",
      "source": [
        "import sqlite3\n",
        "import pandas as pd\n",
        "\n",
        "\n",
        "connect =sqlite3.connect(':memory:')\n",
        "cur=connect.cursor()\n",
        "\n",
        "# table1\n",
        "cur.execute('''\n",
        "CREATE TABLE farmer_register(\n",
        "  registration_number INTEGER PRIMARY KEY,\n",
        "  year_of_entry INTEGER NOT NULL,\n",
        "  number_of_submissions INTEGER NOT NULL,\n",
        "  average_quality TEXT NOT NULL\n",
        ")\n",
        "''')\n",
        "\n",
        "registration_data=[\n",
        "    (204,'2008', '56', 'standard'),\n",
        "    (403, '2010', '50', 'super'),\n",
        "    (312, '2015', '25', 'low'),\n",
        "    (611, '2009', '43', 'semistandard'),\n",
        "    (111, '2018', '20', 'super')\n",
        "]\n",
        "cur.executemany('INSERT INTO farmer_register VALUES (?,?,?,?)', registration_data)\n",
        "\n",
        "\n",
        "#table2\n",
        "cur.execute('''\n",
        "CREATE TABLE farmers_info(\n",
        "    farmer_id INTEGER PRIMARY KEY,\n",
        "    farmer_name TEXT NOT NULL,\n",
        "    farmer_produce TEXT NOT NULL,\n",
        "    farm_size_in_acres INTEGER NOT NULL,\n",
        "    farmer_age INTEGER NOT NULL,\n",
        "    registration_id INTEGER,\n",
        "    FOREIGN KEY(registration_id) REFERENCES farmer_register(registration_number)\n",
        ")\n",
        "''')\n",
        "farmer_data = [\n",
        "    (567, 'mwebaze fred', 'beans', '62', '54', '204'),\n",
        "    (545, 'turyasingura wesley', 'mazie', '45', '45','111'),\n",
        "    (534, 'edwin agaba', 'watermwlon', '60', '57','312'),\n",
        "    (678, 'jadril abdul', 'coffee', '90', '30','403'),\n",
        "    (766, 'benjamin', 'letuce', '67', '67','611')\n",
        "]\n",
        "cur.executemany('INSERT INTO farmers_info VALUES (?,?,?,?,?,?)', farmer_data)\n",
        "connect.commit()\n",
        "\n",
        "def run_query(query):\n",
        "  return pd.read_sql_query(query,connect)\n",
        "\n",
        "print(\"Database successfully created. vamos!\")\n"
      ],
      "metadata": {
