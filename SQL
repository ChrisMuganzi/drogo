    {
      "cell_type": "code",
      "source": [
        "query_join_agg= \"\"\"\n",
        "SELECT\n",
        "farmers_info.farmer_name,\n",
        "farmers_info.farmer_produce,\n",
        "farmer_register.year_of_entry,\n",
        "COUNT(farmer_register.registration_number) AS number_of_farmers,\n",
        "AVG(farmers_info.farm_size_in_acres) AS average_land_size\n",
        "FROM farmer_register INNER JOIN farmers_info ON farmer_register.registration_number=farmers_info.registration_id\n",
        "GROUP BY farmers_info.farmer_name\n",
        "ORDER BY average_land_size ASC;\n",
        "\"\"\"\n",
        "run_query(query_join_agg)"
